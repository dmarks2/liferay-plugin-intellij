	<tag>
		<description>Creates a selectable listing of categories for an entity or group of entities.</description>
		<name>asset-categories-selector</name>
		<tag-class>com.liferay.asset.taglib.servlet.taglib.AssetCategoriesSelectorTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>categoryIds</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The class name of the entity. Available vocabularies are not filtered by this class name.</description>
			<name>className</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The ID (class primary key) of the entity.</description>
			<name>classPK</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The class type of the entity. Available vocabularies are filtered by this class type.</description>
			<name>classTypePK</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>An array of group IDs. Available vocabularies are filtered by these group IDs.</description>
			<name>groupIds</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>hiddenInput</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The component instance's ID.</description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>ignoreRequestValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Whether to show only vocabularies marked as required. The default value is <![CDATA[<code>false</code>]]>.</description>
			<name>showOnlyRequiredVocabularies</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showRequiredLabel</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Whether to restrict selection to one asset category at a time. The default value is <![CDATA[<code>false</code>]]>.</description>
			<name>singleSelect</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Vocabulary visibility types (public or internal) to show. The default value is <![CDATA[<code>AssetVocabularyConstants.VISIBILITY_TYPE_PUBLIC</code>]]>.</description>
			<name>visibilityTypes</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
